{
  "eslint.enable": true,
  "eslint.trace.server": "messages",
  "editor.detectIndentation": false,
  "window.zoomLevel": 0,
  "editor.tabSize": 2,
  "html.format.wrapAttributes": "force",
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "welcomePage",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#c374b2",
    "list.activeSelectionForeground": "#df7ecb",
    "list.inactiveSelectionForeground": "#c374b2",
    "list.highlightForeground": "#df7ecb",
    "scrollbarSlider.activeBackground": "#FF704250",
    "editorSuggestWidget.highlightForeground": "#df7ecb",
    "textLink.foreground": "#df7ecb",
    "progressBar.background": "#df7ecb",
    "pickerGroup.foreground": "#df7ecb",
    "editorGutter.modifiedBackground": "#e26666",
    "gitDecoration.modifiedResourceForeground": "#e26666",
    "tab.activeBorder": "#df7ecb"
  },
  "workbench.colorTheme": "Dracula",
  "vsicons.projectDetection.autoReload": true,
  "workbench.iconTheme": "material-icon-theme",
  "explorer.confirmDragAndDrop": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "files.trimTrailingWhitespace": true,
  "gitlens.keymap": "none",
  "gitlens.codeLens.enabled": false,
  // Extensions
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "typescript": "jsx",
    "typescriptreact": "jsx"
  },
  "[typescript]": {
    "editor.foldingStrategy": "indentation"
  },
  "gitlens.historyExplorer.enabled": true,
  "jest.pathToConfig": "./jest-unit.config.js",
  "java.errors.incompleteClasspath.severity": "ignore",
  "breadcrumbs.enabled": true,
  "vsicons.presets.jsOfficial": true,
  "workbench.fontAliasing": "antialiased",
  "editor.fontWeight": "bold",
  "diffEditor.renderSideBySide": false,
  "togglequotes.chars": ["\"", "'", "`"],
  "githubPullRequests.hosts": [
    {
      "host": "https://github.com",
      "username": "oauth",
      "token": "system"
    }
  ],
  "reactPureToClass.typescript": true,
  "extensions.showRecommendationsOnlyOnDemand": true
}
